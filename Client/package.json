{
  "name": "ideawake",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "custom": "ng serve --disableHostCheck",
    "custom-secure": "ng serve --disableHostCheck --ssl=true",
    "build": "ng build",
    "build:stage": "ng build --configuration=stage",
    "build:prod": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build --prod",
    "build:dev": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build --configuration=dev",
    "build:siriusxm": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build --configuration=siriusxm",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stats": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build --prod --stats-json && webpack-bundle-analyzer dist/ideawake/stats.json",
    "compodoc": "compodoc -p tsconfig.json -s --theme postmark",
    "format": "prettier --write \"src/app/**/*.*\"",
    "format:lint": "tslint 'src/**/*.ts?(x)' && npm run format"
  },
  "private": true,
  "dependencies": {
    "@angular-redux/form": "^10.0.0",
    "@angular-redux/router": "^10.0.0",
    "@angular-redux/store": "^9.0.0",
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@flatfile/adapter": "2.1.1",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
    "@fortawesome/pro-regular-svg-icons": "^5.11.2",
    "@fortawesome/pro-solid-svg-icons": "^5.11.2",
    "@ng-bootstrap/ng-bootstrap": "^5.1.1",
    "@ng-select/ng-select": "^3.7.1",
    "@swimlane/ngx-datatable": "16.0.3",
    "@tinymce/tinymce-angular": "^3.5.0",
    "angulartics2": "^8.3.0",
    "animate.css": "^3.7.2",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.9.3",
    "chartjs-plugin-datalabels": "^0.7.0",
    "flatfile-csv-importer": "0.2.13",
    "lodash": "^4.17.15",
    "mapbox-gl": "^1.10.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "ng-lazyload-image": "^9.0.1",
    "ng2-charts": "2.3.3",
    "ngx-infinite-scroll": "^8.0.1",
    "ngx-skeleton-loader": "^1.2.2",
    "ngx-socket-io": "^3.0.1",
    "ngx-toastr": "^11.1.0",
    "pluralize": "^8.0.0",
    "redux": "4.0.1",
    "rxjs": "~6.4.0",
    "tassign": "^1.0.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "=0.803.22",
    "@angular/cli": "^8.3.29",
    "@angular/compiler-cli": "^8.2.10",
    "@angular/language-service": "^8.2.10",
    "@compodoc/compodoc": "^1.1.11",
    "@types/datatables.net": "^1.10.18",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.3.31",
    "@types/mapbox-gl": "^1.12.0",
    "@types/node": "~8.9.4",
    "@types/react": "^16.9.17",
    "@types/redux-logger": "^3.0.7",
    "codelyzer": "^5.2.1",
    "gulp": "^4.0.2",
    "husky": "^4.0.7",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.0",
    "redux-logger": "^3.0.6",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-bundle-analyzer": "^3.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.*": "prettier --write"
  }
}
